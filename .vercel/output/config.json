{
	"version": 3,
	"routes": [
		{
			"src": "/blog/([^/]+?)",
			"headers": {
				"Location": "/blog/$1/"
			},
			"status": 308
		},
		{
			"src": "/blog/(?:\\/(.*?))?",
			"headers": {
				"Location": "/blog/$1/"
			},
			"status": 308
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/.*",
			"dest": "render"
		}
	]
}